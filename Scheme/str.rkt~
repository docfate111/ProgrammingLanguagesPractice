#reader(lib "htdp-beginner-reader.ss" "lang")((modname str) (read-case-sensitive #t) (teachpacks ()) (htdp-settings #(#t constructor repeating-decimal #f #t none #f () #f)))
(define (insert s i str)
  (string-append (substring s i)
  (string-append str (substring s 0 i))))